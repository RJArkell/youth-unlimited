{
  "name": "gulp-kirby-starter-kit",
  "version": "2.0.0-beta.2",
  "description": "Getting started with Gulp v4 & Kirby v3 in no-time",
  "author": "S1SYPHOS <hello@twobrain.io>",
  "homepage": "https://github.com/S1SYPHOS/Gulp-Kirby-Starter-Kit#readme",
  "main": "Gulpfile.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/S1SYPHOS/Gulp-Kirby-Starter-Kit.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/S1SYPHOS/Gulp-Kirby-Starter-Kit/issues"
  },
  "keywords": [
    "gulp",
    "kirby",
    "cms",
    "starterkit"
  ],
  "scripts": {
    "start": "gulp",
    "build": "NODE_ENV=production gulp build",
    "deploy": "NODE_ENV=production gulp build && gulp deploy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ssh": "docker exec -it docker_php_1 sh"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "browser-sync": "^2.26.3",
    "child_process": "^1.0.2",
    "del": "^4.0.0",
    "eslint": "^5.15.2",
    "eslint-config-google": "^0.12.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-connect-php": "^1.0.3",
    "gulp-eslint": "^5.0.0",
    "gulp-favicons": "^2.3.0",
    "gulp-filter": "^5.1.0",
    "gulp-flatten": "^0.4.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-newer": "^1.4.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-stylelint": "^8.0.0",
    "gulp-svgmin": "^2.1.0",
    "gulp-svgstore": "^7.0.1",
    "gulp-uglify": "^3.0.2",
    "imagemin-pngquant": "^7.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "vinyl-named": "^1.1.0",
    "webpack-stream": "^5.2.1"
  },
  "stylelint": {
    "extends": "stylelint-config-sass-guidelines",
    "rules": {
      "string-quotes": "double",
      "order/order": null,
      "order/properties-alphabetical-order": null,
      "max-nesting-depth": null
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "amd": true,
      "commonjs": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "max-len": [
        "warn",
        {
          "ignoreComments": true
        }
      ],
      "require-jsdoc": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  },
  "browserslist": [
    "last 3 versions",
    "> 1%",
    "IE >= 9"
  ],
  "dependencies": {
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "body-scroll-lock": "^2.6.4",
    "bootstrap": "^4.4.1",
    "gulp-replace": "^1.0.0",
    "jquery": "^3.4.1",
    "lax.js": "^1.2.5",
    "xwiper": "^2.2.0"
  }
}
